#domain_statuses.panel.panel-default
  .panel-heading.clearfix
    = t(:statuses)
  .table-responsive
    %table.table.table-hover.table-bordered.table-condensed
      %thead
        %tr
          %th{class: 'col-xs-6'}= t(:status)
          %th{class: 'col-xs-6'}= t(:description)
      %tbody
        - @domain.statuses.each do |x|
          %tr
            %td= x
            %td
