.panel.panel-default
  .panel-heading
    %h3.panel-title= t('shared.address')
  .panel-body
    %dl.dl-horizontal
      %dt= t('shared.country')
      %dd= @contact.address_country

      %dt= t('shared.city')
      %dd= @contact.address_city

      %dt= t('shared.street')
      %dd= @contact.address_street

      %dt= t('shared.zip')
      %dd= @contact.address_zip

      %dt= t('shared.org_name')
      %dd= @contact.address_org_name
