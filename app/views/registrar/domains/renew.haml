= render 'registrar/shared/title', name: t(:renew_domain)

.row
  .col-md-8
    .panel.panel-default
      .panel-heading
        %h3.panel-title= t(:result)
      .panel-body
        %dl.dl-horizontal
          %dt= t(:domain_name)
          %dd= @data.css('name').text

          %dt= t(:valid_to)
          %dd= @data.css('exDate').text
